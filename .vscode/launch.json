{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/server/app.js",
            "stopOnEntry": false,
            "args": [
                "--compilers",
                "js:babel-register"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": null
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },
        {
            "name": "Debug Mocha",
            "type": "node",
            "address": "localhost",
            "program": "${workspaceRoot}/server/app.js",
            "stopOnEntry": false,
            "port": 5858,
            "args": [
                "--compilers",
                "js:babel-register"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": null
        }
    ]
}